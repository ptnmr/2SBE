<html>
<head>
<meta charset="utf-8">
<title>Sistema de Solicitação de Banca Examinadora</title>
<link rel="stylesheet" type="text/css" href="style.css" />
</head>

<body>  




<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="container">
    <section id="content">
      <h2>Sistema de Solicitação de Banca Examinadora</h2>
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true %>
    </section>
  </div>

 <div class="container">
    <section id="content">
    <%= f.label :password %><br />
    <%= f.password_field :password, autocomplete: "off" %>
    </section>
  </div>

  
<!--
  <% if devise_mapping.rememberable? -%>
    <div class="field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me %>
    </div>
  <% end -%>
-->


  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
<% end %>

<%= render "users/shared/links" %>

</html>
